#include <iostream>
using namespace std;
// 1D array
int main() {
    float A[] = {1,2,3,4,5,6,7,8,9}, *B, *C;
    B = new float[3 * 3];
    C = new float[3 * 3];
    B[0] = 7; B[1] = 8; B[2] = 9;
    B[3] = 4; B[4] = 5; B[5] = 6;
    B[6] = 1; B[7] = 2; B[8] = 3;

    for (int i = 0; i < 9; ++i) {
        C[i] = 0.0;
    }

    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            for (int k = 0; k < 3; k++) {
                C[3 * i + j] += A[3 * i + k] * B[3 * k + j];
            }
        }
    }

    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            cout << C[3 * i + j] << " ";
        }
        cout << endl;
    }
    delete[] B;
    delete[] C;



#include <iostream>
using namespace std;
// 2D array
int main() {
    float A[3][3] = { {1,2,3}, {4,5,6}, {7,8,9} }, **B, **C;
    B = new float*[3];
    for (int i = 0; i < 3; ++i) B[i] = new float[3];
    C = new float*[3];
    for (int i = 0; i < 3; ++i) {
        C[i] = new float[3];
        for (int j = 0; j < 3; ++j) {
            C[i][j] = 0.0;
        }
    }
    B[0][0] = 7; B[0][1] = 8; B[0][2] = 9;
    B[1][0] = 4; B[1][1] = 5; B[1][2] = 6;
    B[2][0] = 1; B[2][1] = 2; B[2][2] = 3;

    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            for (int k = 0; k < 3; k++) {
                C[i][j] += A[i][k] * B[k][j];
            }
        }
    }

    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            cout << C[i][j] << " ";
        }
        cout << endl;
    }
    return 0;
}
